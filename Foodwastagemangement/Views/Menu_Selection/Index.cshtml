@model IEnumerable<Foodwastagemangement.Models.Menu_Selection>

@{
    ViewBag.Title = "Index";
}
@Html.Raw(TempData["Msg"])

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Event_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cat_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Member_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Event_Organizer_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Event_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cat_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event_Organizer_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.S_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.S_Id }) |
